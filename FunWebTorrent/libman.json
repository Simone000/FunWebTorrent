{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "jquery@3.6.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@3.4.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "provider": "cdnjs",
      "library": "knockout@3.5.1",
      "destination": "wwwroot/lib/knockout/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery.blockUI@2.70",
      "destination": "wwwroot/lib/jquery.blockUI/"
    },
    {
      "provider": "jsdelivr",
      "library": "@adminarchitect/bootstrap-datepicker@1.7.1",
      "destination": "wwwroot/lib/adminarchitect/bootstrap-datepicker/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js"
      ]
    }
  ]
}